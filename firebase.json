{
  "hosting": {
    "source": ".",
    "frameworksBackend": {
      "region": "asia-east1",
      "secrets": [
        "openai-api-key", 
        "firebase-admin-client-email",
        "firebase-admin-private-key"
        ]
      }
    },
  "apphosting": {
    "backendId": "novasinger-ai-backend",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }

}
